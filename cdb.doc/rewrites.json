[
  {
    "from": "js/MathJax/*",
    "to": "../app/components/MathJax/*"
  },
  {
    "from": "js/jquery/*",
    "to": "../js/components/jquery/*"
  },
  {
    "from": "js/foundation-sites/*",
    "to": "../js/components/foundation-sites/*"
  },
  {
    "from": "css/foundation-sites/*",
    "to": "../js/components/foundation-sites/*"
  },
  {
    "from": "js/what-input/*",
    "to": "../js/components/what-input/*"
  },
  {
    "from": "js/*",
    "to": "js/*"
  },
  {
    "from": "css/*",
    "to": "css/*"
  },
  {
    "from": "login/*",
    "to": "../../../_users/*"
  },
  {
    "from": "session",
    "to": "../../../_session"
  },
  {
    "from": "",
    "to": "_show/accueil"
  },
  {
    "from": "doc",
    "to": "_show/docedit"
  },
  {
    "from": "doc/:id",
    "to": "_show/doc/:id"
  },
  {
    "from": "doc/:id/edit",
    "to": "_show/docedit/:id"
  },
  {
    "from": "update/:id",
    "to": "_update/doc/:id"
  },
  {
    "from": "update",
    "to": "_update/doc"
  },
  {
    "from": "prof",
    "to": "_show/prof"
  },
  {
    "from": "exams",
    "to": "_list/exams/byexam",
    "query": {
      "group_level": "1"
    }
  },
  {
    "from": "exam/:exam",
    "to": "_list/exam/byexam",
    "query": {
      "reduce": "false",
      "startkey": [":exam"],
      "endkey": [":exam", "\ufff0"]
    }
  },
  {
    "from": "exam/:exam/expanded",
    "to": "_list/examexpanded/byexam",
    "query": {
      "reduce": "false",
      "include_docs": "true",
      "startkey": [":exam"],
      "endkey": [":exam", "\ufff0"]
    }
  },
  {
    "from": "prof/:prof/questions",
    "to": "_list/profdocs/profdocs",
    "query": {
      "startkey": [":prof"],
      "endkey": [":prof", "\ufff0"]
    }
  },
  {
    "from": "verifymulti/:question/:answer",
    "to": "_view/verifychoice",
    "query": {
      "startkey": [":question", ":answer"],
      "endkey": [":question", "\ufff0"],
      "limit": "1"
    },
    "formats": {
      "answer": "int"
    }
  }
]
